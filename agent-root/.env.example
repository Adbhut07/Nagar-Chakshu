GOOGLE_API_KEY=
REDDIT_CLIENT_ID=
REDDIT_CLIENT_SECRET=
REDDIT_USER_AGENT=
GOOGLE_APPLICATION_CREDENTIALS=
BASE_API_URL=
GOOGLE_GEOCODING_API_KEY=

# Agent-Root Server Configuration
SPEAKER_A2A_HOST=127.0.0.1
SPEAKER_A2A_PORT=8003
LOG_LEVEL=info

# CORS Configuration
# Set to "true" to allow all origins (development only)
CORS_ALLOW_ALL=false

# Comma-separated list of allowed origins for production
# Example: https://yourdomain.com,https://www.yourdomain.com,https://staging.yourdomain.com
ALLOWED_ORIGINS=

# Example production configuration:
# ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com,https://api.yourdomain.com

# For Google Compute Engine deployment, you might also need to include:
# - Your GCE external IP address
# - Your custom domain pointing to the GCE instance
# - Any load balancer URLs

# Example GCE configuration:
# ALLOWED_ORIGINS=https://yourdomain.com,http://YOUR_GCE_EXTERNAL_IP,https://YOUR_GCE_EXTERNAL_IP